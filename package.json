{
  "name": "word-grid",
  "description": "Mobile-platform word search game",
  "version": "0.1.0",
  "main": "src/",
  "license": "UNLICENSED",
  "author": {
    "name": "Wolf Hatch"
  },
  "engines": {
    "node": ">= 7.9.0"
  },
  "scripts": {
    "start": "yarn run run ",
    "poststart": "yarn run deploy",
    "deploy": "react-native run-android",
    "prerun": "yarn run kill && adb reverse tcp:3030 tcp:3030",
    "run": "nohup react-native start >/vagrant/react-native.log 2>&1 </dev/null &",
    "prekill": "watchman watch-del-all",
    "kill": "if [ \"$(pgrep -af '/vagrant/node_modules/.bin/react-native' | cut -d\" \" -f1)\" ]; then kill `pgrep -af '/vagrant/node_modules/.bin/react-native' | cut -d\" \" -f1`; fi",
    "log-native": "tail -f -n 50 /vagrant/react-native.log",
    "log-android": "adb logcat"
  },
  "dependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.2.2",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^7.0.0",
    "mobx": "^3.1.9",
    "mobx-react": "^4.1.8",
    "prop-types": "^15.5.10",
    "react": "16.0.0-alpha.6",
    "react-native": "^0.44.0",
    "react-native-button": "^1.8.2",
    "react-native-checkbox": "^1.1.0",
    "react-native-loading-spinner-overlay": "^0.5.0",
    "react-native-modalbox": "^1.3.9",
    "react-native-radio-buttons": "^0.14.0",
    "react-native-sqlite-storage": "^3.3.1",
    "react-native-vector-icons": "^4.2.0",
    "react-navigation": "^1.0.0-beta.11"
  }
}
